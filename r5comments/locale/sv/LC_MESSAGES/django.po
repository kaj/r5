#: forms.py:51
#, python-format
msgid "\"%s\" in a name looks like spam, sorry."
msgstr "\"%s\" i ett namn ser ut som spam.  Beklagar."

#: forms.py:63
msgid "Please use an unshortened url."
msgstr "Använd en oförkortad url."

#: forms.py:67
#, python-format
msgid "I get to much spam from %s, sorry."
msgstr "Jag får för mycket spam från %s.  Beklagar."

#: forms.py:75
#, python-format
msgid "Please use a http or https url, not %s."
msgstr "Använd en http- eller https-url, inte %s."

#: forms.py:86
#, python-format
msgid "Failed to check url %(url)s: %(msg)s."
msgstr "Kunde inte kontrollera urlen %(url)s: %(msg)s."

#: forms.py:96
#, python-format
msgid "Please use a direct url (%(url)s redirects to %(target)s)"
msgstr "Vänligen använd en direkt url (%(url)s redirectar till %(target)s)"

#: forms.py:100
#, python-format
msgid "Your url returns %(code)s %(msg)s"
msgstr "Din url returnerar %(code)s %(msg)s"

#: forms.py:108
#, python-format
msgid "\"%s\" is used in spam, sorry."
msgstr "\"%s\" förekommer i spam.  Beklagar."

